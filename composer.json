{
    "name": "php-yang/configuration",
    "description": "Yang's configuration.",
    "keywords": [
        "php",
        "yang",
        "config",
        "configuration",
		"environment"
    ],
    "homepage": "http://github.com/php-yang/configuration",
    "require": {
        "php": ">=5.4",
        "vlucas/phpdotenv": "~2.5",
        "php-yang/core": "~1.0"
    },
    "suggest": {
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "pengzhile",
            "email": "pengzhile@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Yang\\Configuration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yang\\Configuration\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
